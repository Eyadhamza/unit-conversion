{
    "name": "theprofessor/unit-conversion",
    "description": "perform unit conversion in php",
    "keywords": [
        "theprofessor",
        "unit-conversion"
    ],
    "homepage": "https://github.com/theprofessor/unit-conversion",
    "license": "MIT",
    "authors": [
        {
            "name": "Eyad Hamza",
            "email": "EyadHamza0@outlook.com",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "Theprofessor\\UnitConversion\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Theprofessor\\UnitConversion\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
